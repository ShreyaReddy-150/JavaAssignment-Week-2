package p1;

public class Main {

	public static void main(String[] args) {
		
		centralizedDB db1=new centralizedDB("Database One", 1200.0,5.00, 1500.0);
		System.out.println(db1.toString());
		
		System.out.println("-------------------");
		
		distributedDB db2 = new distributedDB("Database Two", 2000.0,7.5, 100, 12.0);
		System.out.println(db2.toString());
		
		System.out.println("-------------------");
		
		HomogeneousDB db3 = new HomogeneousDB("Database Three", 2000,7.5, 100, 14.0);
		System.out.println(db3.toString());
		
		System.out.println("-------------------");
		
		HeterogeneousDB db4 = new HeterogeneousDB("Database Four", 2000,7.5, 100, 14.0);
		System.out.println(db4.toString());
		
		System.out.println("-------------------");
		
		System.out.println("Number of Objects created: "+db4.getCount());
		
	}

}


